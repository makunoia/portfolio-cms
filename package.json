{
  "name": "portfolio-site",
  "description": "A portfolio site made with Payload CMS 3.0 and Next.JS 15",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devturbo": "cross-env NODE_OPTIONS=--no-deprecation next dev --turbo",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "prebuild": "node set-build-date.js",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && npm build",
    "generate:types": "payload generate:types"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.609.0",
    "@payloadcms/db-mongodb": "3.0.0-beta.84",
    "@payloadcms/db-postgres": "3.0.0-beta.84",
    "@payloadcms/next": "3.0.0-beta.84",
    "@payloadcms/richtext-lexical": "3.0.0-beta.84",
    "@payloadcms/richtext-slate": "3.0.0-beta.84",
    "@payloadcms/storage-s3": "3.0.0-beta.84",
    "@payloadcms/ui": "3.0.0-beta.84",
    "@radix-ui/react-accordion": "^1.2.0",
    "babel-plugin-react-compiler": "^0.0.0-experimental-592953e-20240517",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cobe": "^0.6.3",
    "cross-env": "^7.0.3",
    "framer-motion": "^12.0.0-alpha.0",
    "globe.gl": "^2.32.4",
    "graphql": "^16.8.1",
    "next": "15.0.0-canary.104",
    "next-themes": "^0.3.0",
    "payload": "3.0.0-beta.84",
    "react": "19.0.0-rc-06d0b89e-20240801",
    "react-dom": "19.0.0-rc-06d0b89e-20240801",
    "sharp": "0.32.6",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@next/env": "^15.0.0-canary.104",
    "@radix-ui/react-form": "^0.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@swc/core": "^1.5.28",
    "@types/express": "^4.17.9",
    "@types/node": "^20.14.9",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "@types/three": "^0.165.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.0.11",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.0-canary.104",
    "lucide-react": "^0.395.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "5.5.4"
  },
  "pnpm": {
    "packageManager": "pnpm@9.4.0",
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.0",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0"
  }
}
